<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Founder Images Preview - ISN.BIZ</title>
    <style>
        * {
            margin: 0;
            padding: 0;
            box-sizing: border-box;
        }

        body {
            font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', sans-serif;
            background: #1a1a1a;
            color: #fff;
            padding: 40px 20px;
        }

        .container {
            max-width: 1400px;
            margin: 0 auto;
        }

        h1 {
            text-align: center;
            margin-bottom: 10px;
            color: #1E9FF2;
            font-size: 2.5rem;
        }

        .subtitle {
            text-align: center;
            color: #888;
            margin-bottom: 50px;
            font-size: 1.1rem;
        }

        .category {
            margin-bottom: 60px;
        }

        .category-header {
            display: flex;
            align-items: center;
            margin-bottom: 25px;
            padding-bottom: 15px;
            border-bottom: 2px solid #333;
        }

        .category-title {
            font-size: 1.8rem;
            color: #5FDFDF;
            margin-right: 15px;
        }

        .category-count {
            background: #333;
            padding: 5px 12px;
            border-radius: 20px;
            font-size: 0.9rem;
            color: #aaa;
        }

        .grid {
            display: grid;
            grid-template-columns: repeat(auto-fill, minmax(280px, 1fr));
            gap: 20px;
        }

        .image-card {
            background: #2a2a2a;
            border-radius: 12px;
            overflow: hidden;
            transition: transform 0.3s ease, box-shadow 0.3s ease;
            cursor: pointer;
        }

        .image-card:hover {
            transform: translateY(-5px);
            box-shadow: 0 10px 30px rgba(30, 159, 242, 0.3);
        }

        .image-card img {
            width: 100%;
            height: 300px;
            object-fit: cover;
            display: block;
        }

        .image-info {
            padding: 15px;
        }

        .image-name {
            font-weight: 600;
            color: #fff;
            margin-bottom: 5px;
        }

        .image-size {
            font-size: 0.85rem;
            color: #888;
        }

        .stats {
            display: grid;
            grid-template-columns: repeat(auto-fit, minmax(200px, 1fr));
            gap: 20px;
            margin-bottom: 50px;
            text-align: center;
        }

        .stat-card {
            background: linear-gradient(135deg, #1E9FF2 0%, #5FDFDF 100%);
            padding: 25px;
            border-radius: 12px;
        }

        .stat-number {
            font-size: 3rem;
            font-weight: 700;
            margin-bottom: 5px;
        }

        .stat-label {
            font-size: 0.95rem;
            opacity: 0.9;
        }

        .founder-name {
            display: inline-block;
            background: #1E9FF2;
            color: white;
            padding: 3px 10px;
            border-radius: 4px;
            font-size: 0.8rem;
            margin-right: 5px;
            text-transform: uppercase;
            font-weight: 600;
        }

        /* Modal for full-size view */
        .modal {
            display: none;
            position: fixed;
            z-index: 1000;
            left: 0;
            top: 0;
            width: 100%;
            height: 100%;
            background: rgba(0, 0, 0, 0.95);
            align-items: center;
            justify-content: center;
        }

        .modal.active {
            display: flex;
        }

        .modal img {
            max-width: 90%;
            max-height: 90%;
            object-fit: contain;
        }

        .close-modal {
            position: absolute;
            top: 30px;
            right: 40px;
            font-size: 3rem;
            color: white;
            cursor: pointer;
            z-index: 1001;
        }
    </style>
</head>
<body>
    <div class="container">
        <h1>ISN.BIZ Founder Images</h1>
        <p class="subtitle">44 Professional Images Generated • February 2, 2026</p>

        <div class="stats">
            <div class="stat-card">
                <div class="stat-number">44</div>
                <div class="stat-label">Total Images</div>
            </div>
            <div class="stat-card">
                <div class="stat-number">4</div>
                <div class="stat-label">Founders</div>
            </div>
            <div class="stat-card">
                <div class="stat-number">5</div>
                <div class="stat-label">Categories</div>
            </div>
        </div>

        <!-- Headshots with Background -->
        <div class="category">
            <div class="category-header">
                <h2 class="category-title">Professional Headshots</h2>
                <span class="category-count">4 images</span>
            </div>
            <div class="grid">
                <div class="image-card" onclick="showModal('assets/founders/headshots_with_bg/alicia_headshot.png')">
                    <img src="assets/founders/headshots_with_bg/alicia_headshot.png" alt="Alicia Headshot">
                    <div class="image-info">
                        <div class="image-name"><span class="founder-name">Alicia</span> Professional Headshot</div>
                    </div>
                </div>
                <div class="image-card" onclick="showModal('assets/founders/headshots_with_bg/bri_headshot.png')">
                    <img src="assets/founders/headshots_with_bg/bri_headshot.png" alt="Bri Headshot">
                    <div class="image-info">
                        <div class="image-name"><span class="founder-name">Bri</span> Professional Headshot</div>
                    </div>
                </div>
                <div class="image-card" onclick="showModal('assets/founders/headshots_with_bg/jonathan_headshot.png')">
                    <img src="assets/founders/headshots_with_bg/jonathan_headshot.png" alt="Jonathan Headshot">
                    <div class="image-info">
                        <div class="image-name"><span class="founder-name">Jonathan</span> Professional Headshot</div>
                    </div>
                </div>
                <div class="image-card" onclick="showModal('assets/founders/headshots_with_bg/lilly_headshot.png')">
                    <img src="assets/founders/headshots_with_bg/lilly_headshot.png" alt="Lilly Headshot">
                    <div class="image-info">
                        <div class="image-name"><span class="founder-name">Lilly</span> Professional Headshot</div>
                    </div>
                </div>
            </div>
        </div>

        <!-- Headshots No Background -->
        <div class="category">
            <div class="category-header">
                <h2 class="category-title">Headshots - No Background</h2>
                <span class="category-count">4 images</span>
            </div>
            <div class="grid">
                <div class="image-card" onclick="showModal('assets/founders/headshots_no_bg/alicia_headshot_no_bg.png')">
                    <img src="assets/founders/headshots_no_bg/alicia_headshot_no_bg.png" alt="Alicia No BG">
                    <div class="image-info">
                        <div class="image-name"><span class="founder-name">Alicia</span> No Background</div>
                    </div>
                </div>
                <div class="image-card" onclick="showModal('assets/founders/headshots_no_bg/bri_headshot_no_bg.png')">
                    <img src="assets/founders/headshots_no_bg/bri_headshot_no_bg.png" alt="Bri No BG">
                    <div class="image-info">
                        <div class="image-name"><span class="founder-name">Bri</span> No Background</div>
                    </div>
                </div>
                <div class="image-card" onclick="showModal('assets/founders/headshots_no_bg/jonathan_headshot_no_bg.png')">
                    <img src="assets/founders/headshots_no_bg/jonathan_headshot_no_bg.png" alt="Jonathan No BG">
                    <div class="image-info">
                        <div class="image-name"><span class="founder-name">Jonathan</span> No Background</div>
                    </div>
                </div>
                <div class="image-card" onclick="showModal('assets/founders/headshots_no_bg/lilly_headshot_no_bg.png')">
                    <img src="assets/founders/headshots_no_bg/lilly_headshot_no_bg.png" alt="Lilly No BG">
                    <div class="image-info">
                        <div class="image-name"><span class="founder-name">Lilly</span> No Background</div>
                    </div>
                </div>
            </div>
        </div>

        <!-- Group Photos -->
        <div class="category">
            <div class="category-header">
                <h2 class="category-title">Team Group Photos</h2>
                <span class="category-count">4 images</span>
            </div>
            <div class="grid">
                <div class="image-card" onclick="showModal('assets/founders/group_photos/team_meeting.png')">
                    <img src="assets/founders/group_photos/team_meeting.png" alt="Team Meeting">
                    <div class="image-info">
                        <div class="image-name">Team Meeting</div>
                    </div>
                </div>
                <div class="image-card" onclick="showModal('assets/founders/group_photos/team_presentation.png')">
                    <img src="assets/founders/group_photos/team_presentation.png" alt="Team Presentation">
                    <div class="image-info">
                        <div class="image-name">Team Presentation</div>
                    </div>
                </div>
                <div class="image-card" onclick="showModal('assets/founders/group_photos/team_casual.png')">
                    <img src="assets/founders/group_photos/team_casual.png" alt="Team Casual">
                    <div class="image-info">
                        <div class="image-name">Team Casual</div>
                    </div>
                </div>
                <div class="image-card" onclick="showModal('assets/founders/group_photos/team_brainstorm.png')">
                    <img src="assets/founders/group_photos/team_brainstorm.png" alt="Team Brainstorm">
                    <div class="image-info">
                        <div class="image-name">Team Brainstorming</div>
                    </div>
                </div>
            </div>
        </div>

        <!-- Corporate Photos - showing sample, full list would be too long -->
        <div class="category">
            <div class="category-header">
                <h2 class="category-title">Corporate Activity Photos</h2>
                <span class="category-count">16 images</span>
            </div>
            <p style="color: #888; margin-bottom: 20px; font-style: italic;">
                Presenting • Working • Collaborating • Analyzing (4 scenarios × 4 founders)
            </p>
            <div class="grid" id="corporate-grid"></div>
        </div>

        <!-- Casual Variants -->
        <div class="category">
            <div class="category-header">
                <h2 class="category-title">Casual Lifestyle Variants</h2>
                <span class="category-count">16 images</span>
            </div>
            <p style="color: #888; margin-bottom: 20px; font-style: italic;">
                Coffee Shop • Outdoor • Casual Meeting • Brainstorming (4 scenarios × 4 founders)
            </p>
            <div class="grid" id="casual-grid"></div>
        </div>
    </div>

    <!-- Modal for full-size view -->
    <div class="modal" id="modal" onclick="hideModal()">
        <span class="close-modal">&times;</span>
        <img id="modal-img" src="" alt="Full size">
    </div>

    <script>
        // Generate corporate photos grid
        const founders = ['alicia', 'bri', 'jonathan', 'lilly'];
        const corporateActivities = ['presenting', 'working', 'collaborating', 'analyzing'];
        const corporateGrid = document.getElementById('corporate-grid');

        founders.forEach(founder => {
            corporateActivities.forEach(activity => {
                const path = `assets/founders/corporate_photos/${founder}_${activity}.png`;
                const card = document.createElement('div');
                card.className = 'image-card';
                card.onclick = () => showModal(path);
                card.innerHTML = `
                    <img src="${path}" alt="${founder} ${activity}">
                    <div class="image-info">
                        <div class="image-name">
                            <span class="founder-name">${founder}</span>
                            ${activity.charAt(0).toUpperCase() + activity.slice(1)}
                        </div>
                    </div>
                `;
                corporateGrid.appendChild(card);
            });
        });

        // Generate casual variants grid
        const casualScenarios = ['coffee', 'outdoor', 'casual_meeting', 'brainstorming'];
        const casualGrid = document.getElementById('casual-grid');

        founders.forEach(founder => {
            casualScenarios.forEach(scenario => {
                const path = `assets/founders/casual_variants/${founder}_${scenario}.png`;
                const card = document.createElement('div');
                card.className = 'image-card';
                card.onclick = () => showModal(path);
                card.innerHTML = `
                    <img src="${path}" alt="${founder} ${scenario}">
                    <div class="image-info">
                        <div class="image-name">
                            <span class="founder-name">${founder}</span>
                            ${scenario.replace('_', ' ').split(' ').map(w => w.charAt(0).toUpperCase() + w.slice(1)).join(' ')}
                        </div>
                    </div>
                `;
                casualGrid.appendChild(card);
            });
        });

        // Modal functions
        function showModal(src) {
            document.getElementById('modal-img').src = src;
            document.getElementById('modal').classList.add('active');
        }

        function hideModal() {
            document.getElementById('modal').classList.remove('active');
        }

        // Close modal on Escape key
        document.addEventListener('keydown', (e) => {
            if (e.key === 'Escape') hideModal();
        });
    </script>
</body>
</html>
